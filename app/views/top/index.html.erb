<div class="panel panel-default">
    <div class="panel-heading">
        <% if $keyword.present? %>
          <h2 class="panel-title"><%= @title_info %></h2>
        <% else %>
          <h2 class="panel-title"><%= @title_info %></h2>
             <%= render 'toppagelink' %>
        <% end %>
    </div>
    <div class="panel-body">
        <%= render 'listshow' %>
    </div>
    <div class="panel-footer">
        <% if $keyword.present? %>
           <%= link_to " Homeに戻る" , root_path %>
        <% else %>
           <%= render 'toppagelink' %>
        <% end %>
    </div>
</div>


